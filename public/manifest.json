{
  "manifest_version": 3,
  "name": "NgOneTab",
  "version": "0.0.7",
  "description": "NgOneTab - New One Tab Pro Extension!",
  "options_page": "src/options/index.html",
  "background": {
    "service_worker": "src/background/index.js"
  },
  "action": {
    "icons": {
      "16": "icon-34.png",
      "48": "icon-34.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "src/content/index.js"
      ]
    }
  ],
  "commands": {
    "open-omni": {
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "mac": "Command+Shift+Z"
      },
      "description": "Open command menu"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icon-128.png",
        "icon-34.png",
        "*.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}